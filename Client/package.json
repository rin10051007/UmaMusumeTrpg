{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:Common": "ng build Common --configuration development",
    "build:SystemControl": "ng build SystemControl --configuration development --base-href=/SystemControl/",
    "build:UmaMusumeControl": "ng build UmaMusumeControl --configuration development",
    "build:AuthControl": "ng build AuthControl --configuration development",
    "build": "run-p build:*",
    "watch:Common": "ng build Common --watch --configuration development",
    "watch:SystemControl": "ng build SystemControl --watch --configuration development",
    "watch:UmaMusumeControl": "ng build UmaMusumeControl --watch --configuration development",
    "watch:AuthControl": "ng build AuthControl --watch --configuration development",
    "watch:App": "run-p watch:SystemControl watch:UmaMusumeControl watch:AuthControl",
    "watch": "run-p watch:Common timeout:*",
    "prod:Common": "ng build Common --watch --configuration production",
    "prod:SystemControl": "ng build SystemControl --watch --configuration production",
    "prod:UmaMusumeControl": "ng build UmaMusumeControl --watch --configuration production",
    "prod:AuthControl": "ng build AuthControl --watch --configuration production",
    "prod": "run-p prod:*",
    "test": "ng test",
    "timeout": "timeout /t 10",
    "timeout:App": "run-s timeout watch:App"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^15.2.1",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/material": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@auth0/angular-jwt": "^5.1.2",
    "bootstrap-icons": "^1.10.3",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@types/jasmine": "~4.3.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.9.5"
  }
}
